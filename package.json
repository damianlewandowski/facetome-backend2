{
  "name": "@tsed/socketio-example",
  "version": "1.0.0",
  "description": "Here an example project with SocketIO and Ts.ED framework.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run clean && npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "start:prod": "npm run build && cross-env NODE_ENV=production node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.44.10",
    "@tsed/core": "5.44.10",
    "@tsed/di": "5.44.10",
    "@tsed/socketio": "5.44.10",
    "@tsed/swagger": "5.44.10",
    "@tsed/testing": "5.44.10",
    "@types/swagger-schema-official": "2.0.20",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "cookie-parser": "1.4.4",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "node-uuid": "^1.4.8",
    "socket.io": "2.3.0",
    "ts-json-properties": "1.2.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/chai-as-promised": "7.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.3",
    "@types/http-proxy": "^1.16.2",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.1",
    "@types/request-promise": "^4.1.41",
    "@types/sinon": "7.5.2",
    "@types/sinon-chai": "3.2.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "7.1.0",
    "nodemon": "1.19.1",
    "nyc": "15.0.0",
    "rimraf": "^2.6.2",
    "sinon": "9.0.1",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "8.6.2",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  }
}
